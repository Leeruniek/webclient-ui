{
  "modules": true,
  "plugins": {
    "postcss-preset-env": {
      stage: 3,
      features: {
        'color-mod-function': { unresolved: 'warn' }
      }
    },
    "postcss-custom-properties": {
      "preserve": false,
      "importFrom": [
        "node_modules/react-toolbox/lib/variables.css",
        "node_modules/react-toolbox/lib/colors.css",
        "node_modules/react-toolbox/lib/ripple/theme.css",
        "src/styles/variables.css",
        ]
    }
  }
}

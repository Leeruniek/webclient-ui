{
  "name": "@leeruniek/webclient-ui",
  "version": "0.3.2",
  "description": "Pure UI components",
  "homepage": "https://github.com/leeruniek/webclient-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leeruniek/webclient-ui.git"
  },
  "bugs": {
    "url": "https://github.com/leeruniek/webclient-ui/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "maintainers": [
    {
      "name": "Ivan Gunchenko",
      "url": "https://github.com/whois42"
    },
    {
      "name": "Andrei Dumitrescu",
      "url": "https://github.com/andreidcm"
    }
  ],
  "main": "dist/components.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTILS------------------------------------": "",
    "clean:build": "rm -rf dist/*",
    "clean": "npm run clean:build && rm -rf ./node_modules && rm -rf ./flow-typed",
    "setup": "npm run clean &&  npm i && flow-typed install && npm audit fix",
    "update": "npm-check -u",
    "----DEV--------------------------------------": "",
    "storybook": "start-storybook",
    "watch": "webpack -d --watch --config ./webpack/webpack.config.js --display-error-details",
    "----BUILD------------------------------------": "",
    "build:css": "postcss src/**/*.css src/**/**/*.css --base src --dir dist",
    "build:babel": "babel src --out-dir dist --ignore src/**/*.test.jsx,src/**/*.stories.js,src/**/*.page.jsx",
    "build": "webpack --mode=production --config ./webpack/webpack.config.js --display-error-details",
    "prepare": "npm run clean:build && npm audit fix && npm test && npm run build",
    "prepublishOnly": "npm run test:cover | coveralls",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "----LINT-------------------------------------": "",
    "lint:css": "stylelint src/**/*.css",
    "lint:flow": "flow",
    "lint:js": "eslint --ext .jsx,.js src",
    "lint": "npm run lint:js && npm run lint:css",
    "----TEST-------------------------------------": "",
    "test": "tape -r @babel/register -r ignore-styles src/**/*.test.jsx",
    "test:cover": "nyc --extension .jsx npm test && nyc report --reporter=text-lcov",
    "tdd": "nodemon --watch src/** --exec \"npm run test | tap-diff\""
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "@leeruniek/functies": "^0.33.0",
    "classnames": "2.2.6",
    "core-js": "^3.0.1",
    "cuid": "^2.1.6",
    "debug": "4.1.1",
    "font-awesome": "4.7.0",
    "raven-js": "^3.27.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-is": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.4.3",
    "react-toolbox": "2.0.0-beta.13",
    "sanitize.css": "8.0.0",
    "src": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.3",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "^7.4.0",
    "@leeruniek/eslint-config": "^4.3.0",
    "@storybook/react": "^5.0.8",
    "babel-eslint": "10.0.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-flowtype-errors": "^4.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^8.0.2",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.97.0",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "5.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.18.10",
    "npm-check": "^5.9.0",
    "nyc": "^14.0.0",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-properties": "^8.0.10",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "1.4.1",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-test-renderer": "^16.8.4",
    "resolve-url-loader": "^3.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^2.1.0",
    "tap-diff": "0.1.1",
    "tape": "^4.10.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  }
}

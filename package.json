{
  "name": "@leeruniek/webclient-ui",
  "version": "0.2.0",
  "description": "Pure UI components",
  "homepage": "https://github.com/leeruniek/webclient-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leeruniek/webclient-ui.git"
  },
  "bugs": {
    "url": "https://github.com/leeruniek/webclient-ui/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "maintainers": [
    {
      "name": "Ivan Gunchenko",
      "url": "https://github.com/whois42"
    },
    {
      "name": "Andrei Dumitrescu",
      "url": "https://github.com/andreidcm"
    }
  ],
  "main": "dist/components.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "----UTILS------------------------------------": "",
    "clean:build": "rm -rf dist/*",
    "clean": "npm run clean:build && rm -rf ./node_modules && rm -rf ./flow-typed",
    "setup": "npm run clean &&  npm i && flow-typed install && npm audit fix",
    "update": "npm-check -u",
    "----DEV--------------------------------------": "",
    "start": "parcel src/index.html",
    "----BUILD------------------------------------": "",
    "build:css": "postcss src/**/*.css --base src --dir dist",
    "build:babel": "babel src --out-dir dist",
    "build": "npm run build:babel && npm run build:css",
    "prepare": "npm run clean:build && npm audit fix && npm test && npm run build",
    "prepublishOnly": "npm run test:cover | coveralls",
    "----LINT-------------------------------------": "",
    "lint:css": "stylelint src/**/*.css",
    "lint:flow": "flow",
    "lint:js": "eslint --ext .jsx,.js src",
    "lint": "npm run lint:js && npm run lint:css",
    "----TEST-------------------------------------": "",
    "test": "tape -r @babel/register -r ignore-styles src/**/*.test.jsx",
    "test:cover": "nyc --extension .jsx npm test && nyc report --reporter=text-lcov",
    "tdd": "nodemon --watch src/** --exec \"npm run test | tap-diff\""
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "@leeruniek/functies": "0.19.4",
    "classnames": "2.2.6",
    "cuid": "2.1.4",
    "debug": "4.1.0",
    "font-awesome": "^4.7.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-router-dom": "4.3.1",
    "react-toolbox": "2.0.0-beta.13",
    "sanitize.css": "8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@leeruniek/eslint-config": "4.0.0",
    "babel-eslint": "10.0.1",
    "coveralls": "^3.0.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.10.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-node": "0.3.2",
    "eslint-plugin-compat": "2.6.3",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-html": "5.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.3.2",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-unicorn": "6.0.1",
    "flow-bin": "0.88.0",
    "flow-typed": "2.5.1",
    "ignore-styles": "5.0.1",
    "nodemon": "1.18.8",
    "npm-check": "5.9.0",
    "nyc": "^13.1.0",
    "parcel-bundler": "1.10.3",
    "postcss": "7.0.6",
    "postcss-cli": "6.0.1",
    "postcss-custom-properties": "8.0.9",
    "postcss-modules": "1.4.1",
    "prettier": "1.15.3",
    "react-custom-scrollbars": "4.2.1",
    "react-test-renderer": "16.6.3",
    "stylelint": "9.9.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "2.0.0",
    "tap-diff": "0.1.1",
    "tape": "4.9.1"
  }
}
